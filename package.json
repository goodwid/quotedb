{
  "name": "quotedb",
  "version": "1.0.0",
  "description": "Mike Callahan's movie quote database API",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "babel-node --presets babel-preset-env",
    "start:watch": "nodemon src/app.js --exec babel-node --presets babel-preset-env",
    "test": "mocha --recursive",
    "test:watch": "mocha --recursive -w",
    "test:int": "mocha test/integration",
    "test:int:watch": "mocha -w test/integration",
    "test:unit": "mocha test/unit",
    "test:unit:watch": "mocha -w test/unit",
    "pretest": "npm run lint",
    "lint": "eslint **/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goodwid/quotedb.git"
  },
  "author": "David Goodwin <goodwid@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/goodwid/quotedb/issues"
  },
  "homepage": "https://github.com/goodwid/quotedb#readme",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.2.0",
    "mongoose": "^5.0.9",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.18.2",
    "mocha": "^5.0.4",
    "nodemon": "^1.17.1"
  }
}
